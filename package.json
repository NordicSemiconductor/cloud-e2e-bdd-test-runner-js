{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner",
  "version": "0.0.0-development",
  "description": "Implementation of a test-runner for end-to-end tests of cloud-native applications using Gherkin features.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "npm run eslint && npm run prettier:lint",
    "eslint": "eslint  --ext .js,.ts \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js.git"
  },
  "keywords": [
    "bdd",
    "end-to-end",
    "testing",
    "serverless",
    "cloud-native",
    "aws"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.421.0",
    "@aws-sdk/client-cognito-identity-provider": "3.421.0",
    "@aws-sdk/client-sqs": "3.421.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.421.0",
    "@aws-sdk/protocol-http": "3.374.0",
    "@aws-sdk/signature-v4": "3.374.0",
    "@types/backoff": "2.5.3",
    "@types/chai": "4.3.6",
    "@types/glob": "8.1.0",
    "@types/node-fetch": "3.0.2",
    "aws-sdk": "2.1467.0",
    "backoff": "2.5.0",
    "chai": "4.3.10",
    "chai-subset": "1.6.0",
    "chalk": "4.1.2",
    "fetch-ponyfill": "7.1.0",
    "gherkin": "9.0.0",
    "glob": "8.1.0",
    "graphql": "16.8.1",
    "jsonata": "1.8.6",
    "node-fetch": "3.3.2",
    "toposort": "2.0.2"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.92",
    "@swc/jest": "0.2.29",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "29.5.5",
    "@types/node": "18.18.0",
    "@types/toposort": "2.0.4",
    "jest": "29.7.0"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
