{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner",
  "version": "0.0.0-development",
  "description": "Implementation of a test-runner for end-to-end tests of cloud-native applications using Gherkin features.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "npm run eslint && npm run prettier:lint",
    "eslint": "eslint  --ext .js,.ts \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\"",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js.git"
  },
  "keywords": [
    "bdd",
    "end-to-end",
    "testing",
    "serverless",
    "cloud-native",
    "aws"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-js#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.54.1",
    "@aws-sdk/client-cognito-identity-provider": "3.54.1",
    "@aws-sdk/client-sqs": "3.54.1",
    "@aws-sdk/credential-provider-cognito-identity": "3.54.1",
    "@aws-sdk/protocol-http": "3.54.1",
    "@aws-sdk/signature-v4": "3.54.1",
    "@types/backoff": "2.5.2",
    "@types/chai": "4.3.0",
    "@types/glob": "7.2.0",
    "@types/node-fetch": "3.0.2",
    "aws-sdk": "2.1097.0",
    "backoff": "2.5.0",
    "chai": "4.3.6",
    "chai-subset": "1.6.0",
    "chalk": "4.1.2",
    "fetch-ponyfill": "7.1.0",
    "gherkin": "9.0.0",
    "glob": "7.2.0",
    "graphql": "16.3.0",
    "jsonata": "1.8.6",
    "node-fetch": "3.2.3",
    "toposort": "2.0.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.43",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/toposort": "2.0.3",
    "@types/uuid": "8.3.4",
    "jest": "27.5.1",
    "ts-jest": "27.1.3"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  }
}
