{
  "name": "@bifravst/e2e-bdd-test-runner",
  "version": "0.0.0-development",
  "description": "Implementation of a test-runner for end-to-end tests of cloud-native applications using Gherkin features.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "npm run eslint && npm run prettier:lint",
    "eslint": "eslint  --ext .js,.ts \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "prettier:lint": "prettier --list-different \"./src/**/*.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bifravst/e2e-bdd-test-runner.git"
  },
  "keywords": [
    "BDD",
    "end-to-end",
    "testing",
    "serverless",
    "cloud native",
    "aws"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/bifravst/e2e-bdd-test-runner/issues"
  },
  "homepage": "https://github.com/bifravst/e2e-bdd-test-runner#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.1.0",
    "@aws-sdk/client-cognito-identity-provider": "3.1.0",
    "@aws-sdk/client-sqs": "3.1.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.1.0",
    "@aws-sdk/signature-v4": "3.1.0",
    "@types/backoff": "2.5.1",
    "@types/chai": "4.2.14",
    "@types/glob": "7.1.3",
    "@types/jsonata": "1.5.1",
    "@types/node-fetch": "2.5.7",
    "aws-sdk": "2.824.0",
    "backoff": "2.5.0",
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "chalk": "4.1.0",
    "fetch-ponyfill": "7.0.0",
    "gherkin": "9.0.0",
    "glob": "7.1.6",
    "graphql": "15.4.0",
    "jsonata": "1.8.4",
    "node-fetch": "2.6.1",
    "paho-mqtt": "1.1.0",
    "toposort": "2.0.2",
    "uuid": "8.3.2",
    "ws": "7.4.2"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.86",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/chai-subset": "1.3.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "@types/paho-mqtt": "1.0.4",
    "@types/toposort": "2.0.3",
    "@types/uuid": "8.3.0",
    "@types/ws": "7.4.0",
    "eslint": "7.17.0",
    "husky": "4.3.7",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
